{
  "name": "renovate",
  "version": "1.3.51",
  "private": true,
  "description": "Self-hosted Renovate using the Renovate GitHub Action",
  "homepage": "https://github.com/vidavidorra/renovate#readme",
  "bugs": {
    "url": "https://github.com/vidavidorra/renovate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vidavidorra/renovate.git"
  },
  "license": "GPL-3.0-or-later",
  "author": "Jeroen de Bruijn",
  "type": "module",
  "scripts": {
    "build": "",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{vue,css,less,scss,html,htm,json,md,markdown,yml,yaml}\" --log-level warn",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*.{vue,css,less,scss,html,htm,json,md,markdown,yml,yaml}\" --log-level warn",
    "lint": "npm run format:check && eslint .github/**/*.js",
    "lint:fix": "npm run format && eslint .github/**/*.js --fix",
    "prepare": "husky .github/husky",
    "test": ""
  },
  "commitlint": {
    "extends": [
      "@vidavidorra"
    ]
  },
  "prettier": "@vidavidorra/prettier-config",
  "release": {
    "extends": "@vidavidorra/semantic-release-config"
  },
  "overrides": {
    "@commitlint/config-conventional": {
      "conventional-changelog-conventionalcommits": ">=9.0.0"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "20.3.1",
    "@vidavidorra/commitlint-config": "7.1.11",
    "@vidavidorra/eslint-config": "1.0.3",
    "@vidavidorra/prettier-config": "1.0.6",
    "@vidavidorra/semantic-release-config": "1.0.4",
    "eslint": "9.39.2",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.7.4",
    "semantic-release": "25.0.2"
  },
  "engines": {
    "node": ">=18"
  }
}
